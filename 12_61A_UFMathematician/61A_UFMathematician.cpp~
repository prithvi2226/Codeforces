#include<iostream>
#include<cstdlib>
#include<cmath>
#include<cstring>
#include<string>

using namespace std;

int main(){

	string A, B;

	cin >> A;
	cin >> B;

	string res;

	int bruh;
	bruh = A.size();

	for(int i = 0; i < bruh; i++){
		if(A[i] == B[i]){
//			cout << 0 << endl;
			res.append(0);
		}
		else{
			//cout << 1 << endl;
			res.append(1);
		}
	}

	cout << res << endl;



	return 0;
}




