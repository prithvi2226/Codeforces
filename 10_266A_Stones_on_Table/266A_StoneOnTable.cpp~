#include<iostream>
#include<string>
#include<cstring>
#include<cstdlib>
#include<cmath>

using namespace std;

int main(){

	string stones[100];

	int i, n;
	cin >> n;

	string s;
	int count = 0;

	cin >> s;

	cout << s[1] << endl;

	for(i = 0; i < n; i++){
		if(s[i] != s[i+1]){
			count += 0;
		}
		else if(s[i] == s[i+1]){
			count ++;
		}
	}
	cout << count << endl;



	return 0;

}




